[executor]
name = "executor1"
listenAddress = ":38301"
publicAddress = "${EXECUTOR1_HOST}:38301"
keyPath = "./keys"
paddleFLRole = 0
paddleFLAddress = "paddlefl-env1:38302"

[executor.httpserver]
switch = "on"
httpPort = ":8511"
allowCros = true

[executor.mode]
    # Define the task execution type, supports 'Proxy' and 'Self', default 'Proxy.'
    type = 'Proxy'
    [executor.mode.Self]
        host = "http://dataowner1.node.com:80"
        keyPath = "./ukeys"

[executor.mpc]
    trainTaskLimit = 100
    predictTaskLimit = 100
    rpcTimeout = 3
    taskLimitTime = 3600

[executor.storage]
    localModelStoragePath = "./models"
    localEvaluationStoragePath = "./evalus"

    # Define the prediction result storage type, support XuperDB and Local, the default is local storage.
    type = 'Local'
    [executor.storage.XuperDB]
        host = "http://dataowner1.node.com:80"
        # The private key is the dataOwner node client private key generated by the executor.
        keyPath = "./ukeys"
        namespace = "paddlempc"
        expiretime = 72
    [executor.storage.Local]
        localPredictStoragePath = "./predictions"

[executor.blockchain]
    type = "${BLOCKCHAIN_TYPE}"
    [executor.blockchain.xchain]
        mnemonic = "${MNEMONIC}"
        contractName = "${CONTRACT_NAME}"
        contractAccount = "XC${CONTRACT_ACCOUNT}@xuper"
        chainAddress = "xchain1.node.com:37101"
        chainName = "xuper"

    [executor.blockchain.fabric]
        configFile = "/home/conf/fabric/config.yaml"
        channelId = "${CHANNEL_NAME}"
        chaincode = "${CONTRACT_NAME}"
        userName = "Admin"
        orgName = "Org1"

[log]
level = "debug"
path = "./logs"
